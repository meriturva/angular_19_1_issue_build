{
  "name": "merturva",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng:build:staging": "ng build meriturva -c=staging && npm run inject:script",
    "ng:build:production": "ng build meriturva -c=production && npm run inject:script",
    "start": "ng serve meriturva",
    "i18n": "ng extract-i18n meriturva --output-path projects/meriturva/src/i18n --out-file messages.it.xlf",
    "i18nKendo": "npx kendo-translate projects/meriturva/src/i18n/messages.it.xlf --locale it-IT",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.2",
    "@angular/cdk": "19.1.0",
    "@angular/common": "19.1.2",
    "@angular/compiler": "19.1.2",
    "@angular/core": "19.1.2",
    "@angular/forms": "19.1.2",
    "@angular/localize": "19.1.2",
    "@angular/platform-browser": "19.1.2",
    "@angular/platform-browser-dynamic": "19.1.2",
    "@angular/router": "19.1.2",
    "@angular/service-worker": "19.1.2",
    "@progress/kendo-angular-intl": "18.0.0",
    "@progress/kendo-angular-tooltip": "18.0.0",
    "hammerjs": "2.0.8",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.3",
    "@angular/cli": "19.1.3",
    "@angular/compiler-cli": "19.1.2",
    "@angular/language-service": "19.1.2",
    "@progress/kendo-angular-messages": "1.59.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.10.7",
    "angular-eslint": "19.0.2",
    "eslint": "9.18.0",
    "ng-packagr": "19.1.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.21.0"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
